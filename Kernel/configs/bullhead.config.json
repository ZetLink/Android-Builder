[
    {
        "kernelSource": {
            "name": "Nexus5X",
            "repo": "https://github.com/mortezadadgar/android_kernel_lge_bullhead",
            "branch": "master",
            "device": "bullhead",
            "defconfig": "shadow_defconfig"
        },
        "toolchains": [
            {
                "repo": "https://github.com/mvaisakh/gcc-arm64",
                "branch": "gcc-master",
                "name": "aarch64",
                "binaryEnv": [
                    "bin"
                ]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "aarch64-elf-gcc",
            "externalCommands": {
                "CROSS_COMPILE": "aarch64-elf-"
            }
        },
        "enableKernelSU": false,
        "enableLXC": true
    }
]